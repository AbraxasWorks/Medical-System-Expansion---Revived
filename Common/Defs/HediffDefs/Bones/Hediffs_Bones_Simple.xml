<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- base -->

	<HediffDef Abstract="True" Name="AddedBodyPart_Bone_Prosthetic" ParentName="AddedBodyPartBase">
		<addedPartProps>
			<solid>true</solid>
			<betterThanNatural>false</betterThanNatural>
			<partEfficiency>0.75</partEfficiency>
		</addedPartProps>
	</HediffDef>

	<ThingDef Abstract="True" Name="BodyPartProsthetic_Bone" ParentName="BodyPartProstheticMakeableBase">
		<thingCategories Inherit="False">
			<li>BonePartsSteel</li>
		</thingCategories>
		<recipeMaker>
			<researchPrerequisite>BoneProstheticsSimple</researchPrerequisite>
		</recipeMaker>
	</ThingDef>

	<RecipeDef Abstract="True" Name="SurgeryInstallBodyPart_Limb_Prosthetic" ParentName="SurgeryInstallBodyPartProstheticBase">
		<surgerySuccessChanceFactor>0.9</surgerySuccessChanceFactor>
		<deathOnFailedSurgeryChance>0.01</deathOnFailedSurgeryChance>
		<skillRequirements>
			<Medicine>5</Medicine>
		</skillRequirements>
		<modExtensions>
			<li Class="MSE2.InstallationRestrictions">
				<onlyOnNatural>true</onlyOnNatural>
			</li>
			<li Class="MSE2.AutoRecipeUsers">
				<bodySizeRange>0.7~1.2</bodySizeRange>
			</li>
		</modExtensions>
	</RecipeDef>

	<!-- ribcage -->

	<HediffDef ParentName="AddedBodyPart_Bone_Prosthetic">
		<defName>SimpleProstheticRibcage</defName>
		<label>prosthetic ribcage</label>
		<labelNoun>a prosthetic ribcage</labelNoun>
		<description>An installed prosthetic ribcage.</description>
		<descriptionHyperlinks>
			<ThingDef>SimpleProstheticRibcage</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>SimpleProstheticRibcage</spawnThingOnRemoved>
	</HediffDef>

	<ThingDef ParentName="BodyPartProsthetic_Bone">
		<defName>SimpleProstheticRibcage</defName>
		<label>prosthetic ribcage</label>
		<description>A ribcage prosthesis. It is inferior to a real ribcage.</description>
		<descriptionHyperlinks>
			<RecipeDef>InstallSimpleProstheticRibcage</RecipeDef>
		</descriptionHyperlinks>
		<costList>
			<Steel>20</Steel>
		</costList>
		<statBases>
			<Mass>1.5</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallBodyPart_Limb_Prosthetic">
		<defName>InstallSimpleProstheticRibcage</defName>
		<label>install prosthetic ribcage</label>
		<description>Install a prosthetic ribcage.</description>
		<descriptionHyperlinks>
			<ThingDef>SimpleProstheticRibcage</ThingDef>
		</descriptionHyperlinks>
		<jobString>Installing prosthetic ribcage.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SimpleProstheticRibcage</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>SimpleProstheticRibcage</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Ribcage</li>
		</appliedOnFixedBodyParts>
		<addsHediff>SimpleProstheticRibcage</addsHediff>
	</RecipeDef>

