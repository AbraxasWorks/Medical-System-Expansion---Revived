<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <SpecialThingFilterDef>
    <defName>AllowProsthesesIncomplete</defName>
    <label>allow incomplete</label>
    <description>Allow prostheses that are missing some sub-parts for the selected version.</description>
    <parentCategory>BodyParts</parentCategory>
    <allowedByDefault>true</allowedByDefault>
    <saveKey>allowProsthesesIncomplete</saveKey>
    <workerClass>MSE2.SpecialThingFilterWorker_ProsthesisIncomplete</workerClass>
  </SpecialThingFilterDef>

  <SpecialThingFilterDef>
    <defName>AllowProsthesesComplete</defName>
    <label>allow complete</label>
    <description>Allow prostheses that are not missing any sub-part.</description>
    <parentCategory>BodyParts</parentCategory>
    <allowedByDefault>true</allowedByDefault>
    <saveKey>allowProsthesesComplete</saveKey>
    <workerClass>MSE2.SpecialThingFilterWorker_ProsthesisComplete</workerClass>
  </SpecialThingFilterDef>

</Defs>