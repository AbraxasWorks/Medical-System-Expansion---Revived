<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Archotech Obliterator -->

	<HediffDef ParentName="MSE_ModuleAddedArchotech">
		<defName>ArchotechObliterator</defName>
		<label>archotech obliterator</label>
		<labelNoun>an archotech obliterator</labelNoun>
		<description>An installed archotech obliterator module.</description>
		<descriptionHyperlinks>
			<ThingDef>ArchotechObliterator</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>ArchotechObliterator</spawnThingOnRemoved>
		<!-- comps added via patch -->
	</HediffDef>

	<ThingDef ParentName="MSE_ModuleArchotech">
		<defName>ArchotechObliterator</defName>
		<label>archotech obliterator</label>
		<description>An archotech weapon module which includes melee combat features based around attrition and enemy capture. With extremely fast servos the fist is able to extend and retract at remarkable speeds, causing a minature shockwave to form infront of the fist. Its internal workings are a mystery to all human minds.</description>
		<descriptionHyperlinks>
			<RecipeDef>InstallArchotechObliterator</RecipeDef>
		</descriptionHyperlinks>
		<thingSetMakerTags>
			<li>RewardSpecial</li>
		</thingSetMakerTags>
		<statBases>
			<MarketValue>18000</MarketValue>
			<Mass>2</Mass>
		</statBases>
		<techHediffsTags>
			<li>AdvancedWeapon</li>
		</techHediffsTags>
	</ThingDef>

	<RecipeDef ParentName="MSE_SurgeryInstallModuleBase">
		<defName>InstallArchotechObliterator</defName>
		<label>install archotech obliterator</label>
		<description>Install an archotech obliterator.</description>
		<descriptionHyperlinks>
			<ThingDef>ArchotechObliterator</ThingDef>
			<HediffDef>ArchotechObliterator</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing archotech obliterator.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ArchotechObliterator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ArchotechObliterator</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Hand</li>
		</appliedOnFixedBodyParts>
		<addsHediff>ArchotechObliterator</addsHediff>
	</RecipeDef>

	<!-- death claw -->

	<HediffDef ParentName="MSE_ModuleAddedArchotech">
		<defName>ArchotechDeathClaw</defName>
		<label>archotech death claw</label>
		<labelNoun>an archotech death claw</labelNoun>
		<description>An installed archotech death claw module.</description>
		<descriptionHyperlinks>
			<ThingDef>ArchotechDeathClaw</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>ArchotechDeathClaw</spawnThingOnRemoved>
		<!-- comps added via patch -->
	</HediffDef>

	<ThingDef ParentName="MSE_ModuleArchotech">
		<defName>ArchotechDeathClaw</defName>
		<label>archotech death claw</label>
		<description>An archotech weapon module built by an archotech. From the arm pertrudes giant spikes and claws which lash out and serrate anything that comes too close. Its internal workings are a mystery to all human minds.</description>
		<thingSetMakerTags>
			<li>RewardSpecial</li>
		</thingSetMakerTags>
		<statBases>
			<MarketValue>18000</MarketValue>
			<Mass>2</Mass>
		</statBases>
		<techHediffsTags>
			<li>AdvancedWeapon</li>
		</techHediffsTags>
	</ThingDef>

	<RecipeDef ParentName="MSE_SurgeryInstallModuleBase">
		<defName>InstallArchotechDeathClaw</defName>
		<label>install archotech death claw</label>
		<description>Install an archotech death claw.</description>
		<descriptionHyperlinks>
			<ThingDef>ArchotechDeathClaw</ThingDef>
			<HediffDef>ArchotechDeathClaw</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing archotech death claw.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ArchotechDeathClaw</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ArchotechDeathClaw</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Arm</li>
		</appliedOnFixedBodyParts>
		<addsHediff>ArchotechDeathClaw</addsHediff>
	</RecipeDef>

</Defs>