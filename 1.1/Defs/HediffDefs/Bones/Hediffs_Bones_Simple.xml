<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!-- abstracts -->

    <HediffDef Name="MSE_Bones_Simple_Base" ParentName="MSE_AddedBodyPartSimple" Abstract="True">
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>0.75</partEfficiency>
            <betterThanNatural>false</betterThanNatural>
		</addedPartProps>
	</HediffDef>
  
	<ThingDef Name="MSE_BodyPartSimple_Bone" ParentName="MSE_BodyPartSimple" Abstract="True">
		<recipeMaker>
			<researchPrerequisite>MSE_BoneReplacement</researchPrerequisite>
		</recipeMaker>
	</ThingDef>

	<RecipeDef Name="MSE_SurgerySimple_Bone" ParentName="MSE_SurgerySimple" Abstract="True">
		<surgerySuccessChanceFactor>0.90</surgerySuccessChanceFactor>
		<researchPrerequisite>MSE_BoneReplacement</researchPrerequisite>
	</RecipeDef>

	<!-- clavicle -->

	<HediffDef ParentName="MSE_AddedBodyPartSimple">
		<defName>SimpleProstheticClavicle</defName>
		<label>prosthetic clavicle</label>
		<labelNoun>a prosthetic clavicle</labelNoun>
		<description>Test. Fix Later</description>
	</HediffDef>

	<ThingDef ParentName="MSE_BodyPartSimple_Bone">
		<defName>SimpleProstheticClavicle</defName>
		<label>prosthetic clavicle</label>
		<description>A clavicle prosthesis. It is inferior to a real clavicle. Requires bone replacement research to install it.</description>
		<costList>
			<Steel>10</Steel>
		</costList>
		<statBases>
			<MarketValue>25</MarketValue>
			<WorkToMake>1200</WorkToMake>
			<Mass>0.3</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="MSE_SurgerySimple_Bone">
		<defName>InstallSimpleProstheticClavicle</defName>
		<label>install prosthetic clavicle</label>
		<description>Install a prosthetic clavicle.</description>
		<jobString>Installing prosthetic clavicle.</jobString>
		<workAmount>1500</workAmount>
		<skillRequirements>
			<Medicine>8</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SimpleProstheticClavicle</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>SimpleProstheticClavicle</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Clavicle</li>
		</appliedOnFixedBodyParts>
		<addsHediff>SimpleProstheticClavicle</addsHediff>
	</RecipeDef>

