<Defs>

    <!-- abstracts -->

    <HediffDef Name="MSE_Bones_Archotech_Base" ParentName="MSE_AddedBodyPartBase_Archotech" Abstract="True">
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.30</partEfficiency>
            <betterThanNatural>true</betterThanNatural>
		</addedPartProps>
	</HediffDef>

    <!-- clavicle -->

	<HediffDef ParentName="MSE_Bones_Archotech_Base">
		<defName>ArchotechClavicle</defName>
		<label>archotech clavicle</label>
		<labelNoun>a archotech clavicle</labelNoun>
		<description>Test. Fix Later</description>
		<spawnThingOnRemoved>ArchotechClavicle</spawnThingOnRemoved>
	</HediffDef>

	<ThingDef ParentName="MSE_BodyPartArchotech">
		<defName>ArchotechClavicle</defName>
		<label>archotech clavicle</label>
		<description>An artifical clavicle built by an archotech. It's harder to damage than plasteel. Even if it is harmed, it repairs itself over time. Requires archotech surgery research to install it.</description>
		<costList>
			<Uranium>5</Uranium>
			<Gold>10</Gold>
			<Plasteel>10</Plasteel>
			<ComponentSpacer>2</ComponentSpacer>
		</costList>
		<statBases>
			<MarketValue>800</MarketValue>
			<WorkToMake>10000</WorkToMake>
			<Mass>0.3</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="MSE_SurgeryArchotech">
		<defName>InstallArchotechClavicle</defName>
		<label>install archotech clavicle</label>
		<description>Install an archotech clavicle.</description>
		<jobString>Installing archotech clavicle.</jobString>
		<workAmount>2000</workAmount>
		<skillRequirements>
			<Medicine>10</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ArchotechClavicle</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>ArchotechClavicle</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Clavicle</li>
		</appliedOnFixedBodyParts>
		<addsHediff>ArchotechClavicle</addsHediff>
	</RecipeDef>

