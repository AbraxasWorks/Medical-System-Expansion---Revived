	<!-- clavicle -->

	<HediffDef ParentName="MSE_Bones_Plasteel_Base">
		<defName>PlasteelClavicle</defName>
		<label>plasteel clavicle</label>
		<labelNoun>a plasteel clavicle</labelNoun>
		<description>Test. Fix Later</description>
		<descriptionHyperlinks><ThingDef>PlasteelClavicle</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>PlasteelClavicle</spawnThingOnRemoved>
	</HediffDef>
    
	<ThingDef ParentName="MSE_BodyPartBionic_Bone">
		<defName>PlasteelClavicle</defName>
		<label>plasteel clavicle</label>
		<description>An advanced artificial clavicle. Requires plasteel bones research to install it.</description>
		<descriptionHyperlinks><RecipeDef>InstallPlasteelClavicle</RecipeDef></descriptionHyperlinks>
		<costList>
			<Plasteel>5</Plasteel>
			<ComponentSpacer>1</ComponentSpacer>
		</costList>
		<statBases>
			<MarketValue>300</MarketValue>
			<WorkToMake>4000</WorkToMake>
			<Mass>0.3</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="MSE_SurgeryBionic_Bone">
		<defName>InstallPlasteelClavicle</defName>
		<label>install plasteel clavicle</label>
		<description>Install a plasteel clavicle.</description>
		<descriptionHyperlinks><ThingDef>PlasteelClavicle</ThingDef></descriptionHyperlinks>
		<jobString>Installing plasteel clavicle.</jobString>
		<workAmount>1500</workAmount>
		<skillRequirements>
			<Medicine>10</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter><categories><li>Medicine</li></categories></filter>
				<count>2</count>
			</li>
			<li>
				<filter><thingDefs><li>PlasteelClavicle</li></thingDefs></filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>PlasteelClavicle</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Clavicle</li>
		</appliedOnFixedBodyParts>
		<addsHediff>PlasteelClavicle</addsHediff>
	</RecipeDef>

