	<HediffDef ParentName="MSE_ImplantHediffArchotech_Brain">
		<defName>AdvancedPainstopper</defName>
		<label>advanced painstopper</label>
		<labelNoun>an advanced painstopper</labelNoun>
		<comps>
			<li Class="OrenoMSE.HediffCompProperties_Painstopper">
				<label>painstopper</label>
				<description>activate/deactivate painstopper that nullify pain.</description>
				<uiIconPath>UI/Icons/Prosthetic/IconPainstopper</uiIconPath>
			</li>
		</comps>
		<stages>
			<li>
				<minSeverity>0.5</minSeverity>
				<painFactor>0</painFactor>
			</li>
			<li>
				<label>disabled</label>
