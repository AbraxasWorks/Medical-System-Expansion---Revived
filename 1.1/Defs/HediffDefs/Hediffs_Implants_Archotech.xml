	<HediffDef ParentName="MSE_ImplantHediffArchotech">
		<defName>MSE_AdvancedEndoskeletonStimulator</defName>
		<label>advanced endoskeleton stimulator</label>
		<labelNoun>an advanced endoskeleton stimulator</labelNoun>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>0.20</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
