<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- foot -->

	<HediffDef ParentName="AddedBodyPartBase">
		<defName>BionicFoot</defName>
		<label>bionic foot</label>
		<labelNoun>a bionic foot</labelNoun>
		<description>An installed bionic foot.</description>
    <descriptionHyperlinks><ThingDef>BionicFoot</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>BionicFoot</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.25</partEfficiency>
      <betterThanNatural>true</betterThanNatural>
		</addedPartProps>
	</HediffDef>

	<ThingDef ParentName="BodyPartBionicBase">
		<defName>BionicFoot</defName>
		<label>bionic foot</label>
		<description>An advanced artificial foot. With its biogel nerve-link and lattice-dust healing system, it is better than a biological foot in almost every way.</description>
    <descriptionHyperlinks><RecipeDef>InstallBionicFoot</RecipeDef></descriptionHyperlinks>
		<costList>
			<Plasteel>9</Plasteel>
			<ComponentSpacer>2</ComponentSpacer>
		</costList>
    <thingSetMakerTags><li>RewardStandardMidFreq</li></thingSetMakerTags>
		<statBases>
			<Mass>2</Mass>
		</statBases>
		<comps>
			<li Class="OrenoMSE.CompProperties_CompIncludedChildParts">
				<standardChildren>
          <li>BionicToe</li>
          <li>BionicToe</li>
          <li>BionicToe</li>
          <li>BionicToe</li>
          <li>BionicToe</li>
        </standardChildren>
			</li>
		</comps>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
		<defName>InstallBionicFoot</defName>
		<label>install bionic foot</label>
		<description>Install a bionic foot.</description>
    <descriptionHyperlinks><ThingDef>BionicFoot</ThingDef></descriptionHyperlinks>
		<jobString>Installing bionic foot.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicFoot</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicFoot</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Foot</li>
		</appliedOnFixedBodyParts>
		<addsHediff>BionicFoot</addsHediff>
	</RecipeDef>

