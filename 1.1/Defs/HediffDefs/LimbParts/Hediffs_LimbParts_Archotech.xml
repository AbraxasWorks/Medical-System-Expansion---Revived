<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- internal support -->

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>ArchotechInternalSupport</defName>
    <label>archotech internal support</label>
    <labelNoun>a archotech internal support</labelNoun>
    <description>An installed archotech internal support.</description>
    <descriptionHyperlinks>
      <ThingDef>ArchotechInternalSupport</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>ArchotechInternalSupport</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <betterThanNatural>true</betterThanNatural>
      <partEfficiency>1.20</partEfficiency>
    </addedPartProps>
  </HediffDef>

  <ThingDef ParentName="BodyPartArchotechBase">
    <defName>ArchotechInternalSupport</defName>
    <label>archotech internal support</label>
    <description>An artificial internal support built by an archotech. Useful replacement to have on hand for when a archotech arm or leg breaks.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallArchotechInternalSupport</RecipeDef>
    </descriptionHyperlinks>
    <thingSetMakerTags>
      <li>RewardStandardMidFreq</li>
    </thingSetMakerTags>
    <statBases>
      <MarketValue>400</MarketValue>
      <Mass>0.3</Mass>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>InstallArchotechInternalSupport</defName>
    <label>install archotech internal support</label>
    <description>Install a archotech internal support.</description>
    <descriptionHyperlinks>
      <ThingDef>ArchotechInternalSupport</ThingDef>
    </descriptionHyperlinks>
    <jobString>Installing archotech internal support.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ArchotechInternalSupport</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ArchotechInternalSupport</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Clavicle</li>
      <li>Humerus</li>
      <li>Radius</li>
      <li>Femur</li>
      <li>Tibia</li>
    </appliedOnFixedBodyParts>
    <addsHediff>ArchotechInternalSupport</addsHediff>
    <modExtensions>
      <li Class="OrenoMSE.PartInstallation.InstallationRestrictions">
        <whitelist>
          <li>ArchotechArm</li>
          <li>ArchotechLeg</li>
        </whitelist>
      </li>
    </modExtensions>
  </RecipeDef>

