		<description>Test. Fix Later</description>
		<stages>
			<li>
				<statOffsets>
					<MeleeDodgeChance>10</MeleeDodgeChance>
					<MeleeHitChance>15</MeleeHitChance>
				</statOffsets>
			</li>
		</stages>
		<spawnThingOnRemoved>MSE_MeleePlatformModule</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
		</addedPartProps>
	</HediffDef>

