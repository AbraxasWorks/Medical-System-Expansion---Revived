	<HediffDef ParentName="MSE_AddedBodyPartArchotechSpecial">
		<defName>AdvancedPowerArm</defName>
		<label>advanced power arm</label>
		<labelNoun>an advanced power arm</labelNoun>
		<description>Test. Fix Later</description>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>fist</label>
						<capacities>
							<li>Blunt</li>
						</capacities>
						<power>24.0</power>
						<cooldownTime>2.0</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
		<spawnThingOnRemoved>AdvancedPowerArm</spawnThingOnRemoved>
		<addedPartProps>
			<isGoodWeapon>true</isGoodWeapon>
			<solid>true</solid>
			<partEfficiency>1.25</partEfficiency>
		</addedPartProps>
	</HediffDef>

