	<!--================ Bone Implants =================-->

	<RecipeDef ParentName="MSE_SurgeryImplantBionic">
		<defName>MSE_InstallAdvancedEndoskeletonStimulator</defName>
		<label>install advanced endoskeleton stimulator</label>
		<description>Install an advanced endoskeleton stimulator.</description>
		<jobString>Installing advanced endoskeleton stimulator.</jobString>
		<workAmount>3200</workAmount>
		<skillRequirements>
			<Medicine>8</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>MSE_AdvancedEndoskeletonStimulator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>MSE_AdvancedEndoskeletonStimulator</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Torso</li>
		</appliedOnFixedBodyParts>
		<addsHediff>MSE_AdvancedEndoskeletonStimulator</addsHediff>
		<modExtensions>
			<li Class="OrenoMSE.MSE_ImplantSystem">
				<isSpecial>true</isSpecial>
			</li>
		</modExtensions>
	</RecipeDef>

