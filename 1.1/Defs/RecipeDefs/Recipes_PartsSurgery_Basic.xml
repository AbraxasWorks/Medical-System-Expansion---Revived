<Defs>

	<RecipeDef ParentName="MSE_SurgeryBasic">
		<defName>InstallPegLeg</defName>
		<label>install peg leg</label>
		<description>Install a peg leg.</description>
		<jobString>Installing peg leg.</jobString>
		<workAmount>1300</workAmount>
		<skillRequirements>
			<Medicine>3</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>PegLeg</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>PegLeg</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Leg</li>
		</appliedOnFixedBodyParts>
		<addsHediff>PegLeg</addsHediff>
		<recipeUsers Inherit="False" />
	</RecipeDef>

