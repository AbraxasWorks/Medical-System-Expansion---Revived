<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Prosthetics and Organ Engineering</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>


                <li Class="PatchOperationAttributeSet">
                    <xpath>/Defs/RecipeDef[@ParentName="MSE_SurgeryBionic_Organ"]</xpath>
                    <attribute>ParentName</attribute>
                    <value>SurgerySynthetic</value>
                </li>

				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[@Name="MSE_SurgeryBionic_Organ" or @ParentName="MSE_SurgeryBionic_Organ"]</xpath> 
				</li>
				
                <li Class="PatchOperationRemove">  
                    <xpath>/Defs/RecipeDef[@Name="MSE_SurgerySimple_Organ"]/researchPrerequisite</xpath>
                </li>



                <li Class="PatchOperationReplace">
                    <xpath>/Defs/RecipeDef[defName="InstallAdvancedBionicArm"]/appliedOnFixedBodyParts</xpath>
                    <value>
                        <appliedOnFixedBodyParts>
                            <li>Arm</li>    
                        </appliedOnFixedBodyParts>
                    </value>
                </li>


			</operations>
		</match>
	</Operation>

</Patch>