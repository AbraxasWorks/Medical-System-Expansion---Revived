<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Prosthetics and Organ Engineering</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

                <!-- denture (is duplicate because of capital letter) -->
                <li Class="PatchOperationRemove">
                    <xpath>Defs/HediffDef[defName="denture"]</xpath>
                </li>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/RecipeDef[defName="CreateDenture"]</xpath>
                </li>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/RecipeDef[addsHediff="denture"]</xpath>
                </li>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/ThingDef[defName="denture"]</xpath>
                </li>


			</operations>
		</match>
	</Operation>

</Patch>