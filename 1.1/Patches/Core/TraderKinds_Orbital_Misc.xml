<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
		<order>Append</order>
		<value>
			<li Class="StockGenerator_Tag">
				<tradeTag>MSE_BionicProstheses</tradeTag>
				<thingDefCountRange>
					<min>1</min>
					<max>2</max>
				</thingDefCountRange>
				<countRange>
					<min>0</min>
					<max>1</max>
				</countRange>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/TraderKindDef[defName="Orbital_PirateMerchant"]/stockGenerators</xpath>
		<order>Append</order>
		<value>
			<li Class="StockGenerator_Tag">
				<tradeTag>MSE_BionicProstheses</tradeTag>
				<thingDefCountRange>
					<min>1</min>
					<max>2</max>
				</thingDefCountRange>
				<countRange>
					<min>-1</min>
					<max>1</max>
				</countRange>
			</li>
		</value>
	</Operation>

</Patch>