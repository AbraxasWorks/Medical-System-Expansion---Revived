<Defs>

	<HediffDef ParentName="MSE_ImplantComplicationHediff_Brain">
		<defName>MSE_JoywireComplication</defName>
		<label>joywire complication</label>
		<stages>
			<li>
				<minSeverity>0.0001</minSeverity>
				<becomeVisible>false</becomeVisible>
			</li>
			<li>
				<label>mild</label>
				<minSeverity>0.0002</minSeverity>
				<painOffset>0.05</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.15</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>moderate</label>
				<minSeverity>0.30</minSeverity>
				<painOffset>0.15</painOffset>
				<vomitMtbDays>1.0</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.30</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>serious</label>
				<minSeverity>0.60</minSeverity>
				<painOffset>0.25</painOffset>
				<partEfficiencyOffset>-0.10</partEfficiencyOffset>
				<vomitMtbDays>0.5</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.50</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>deadly</label>
				<minSeverity>0.85</minSeverity>
				<lifeThreatening>true</lifeThreatening>
				<painOffset>0.45</painOffset>
				<partEfficiencyOffset>-0.30</partEfficiencyOffset>
				<vomitMtbDays>0.3</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.75</offset>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>

